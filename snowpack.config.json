{
  "mount": {
    "src": "/static",
    "public": "/"
  },
  "plugins": [
    [
      "imba-snowpack",
      {
        "entrypoints": ["app-root"]
      }
    ]
  ],
  "installOptions": {
    "sourceMap": true
  },
  "install": [
    "imba",
    "axios",
    "dayjs",
    "dexie",
    "dexie-relationships",
    "flatpickr",
    "uuid",
    "js-convert-case"
  ],
  "devOptions": {
    "minify": false,
    "open": "none",
    "bundle": true
  },
  "buildOptions": {
    "minify": true
  }
}